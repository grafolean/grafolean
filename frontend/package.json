{
  "name": "grafolean",
  "version": "0.1.0",
  "description": "Grafolean - easy to use monitoring system",
  "repository": "https://github.com/grafolean/grafolean/",
  "private": true,
  "dependencies": {
    "formik": "^2.4.6",
    "lodash": "^4.17.21",
    "mathjs": "^13.2.1",
    "moment-timezone": "^0.5.46",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.5.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^5.3.4",
    "react-scripts": "5.0.1",
    "react-sidebar": "^3.0.2",
    "redux": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --max-warnings 0 --ext .js,.ts,.tsx src/ .storybook/",
    "tslint": "eslint --max-warnings 0 --ext .ts,.tsx src/",
    "prettier": "prettier --write \"src/**/*.{js,ts,tsx}\" \".storybook/**/*.{js,ts,tsx}\"",
    "test": "react-scripts test --env=jsdom",
    "storybook": "start-storybook -p 9000 --no-version-updates -c .storybook"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "sass": "^1.80.6",
    "sass-loader": "^16.0.3"
  }
}
